cont\"/\u003e\u003c/td\u003e\r\n    \t\t\t\t\t\t\t\t\t\t\t\t\t\u003ctd\u003e\u003cbutton ng-click=\"delcontacts(n)\" class=\"btn\"\u003e\u003ci class=\"fa fa-trash-o\"\u003e\u003c/i\u003e\u003c/button\u003e\u003c/td\u003e\r\n    \t\t\t\t\t\t\t\t\t\t\t\t\u003c/tr\u003e\r\n    \t\t\t\t\t\t\t\t\t\t\t\u003c/table\u003e\r\n    \t\t\t\t\t\t\t\t\t\t\t\u003cbutton ng-click=\"addcontacts()\" class=\"btn\"\u003e\u003ci class=\"fa fa-plus\"\u003e\u003c/i\u003e\u003c/button\u003e\r\n                                \t\t\t\u003c/div\u003e\r\n                                \t\t\u003c/div\u003e\r\n                                \t\u003c/div\u003e\r\n\t\t\t\t\t\t\t\t\u003c/div\u003e\r\n\t\t\t\t\t\t\t\u003c/form\u003e\r\n\t\t\t\t\t\t\u003c/div\u003e\r\n\t\t\t\t\t\u003c/div\u003e\r\n\t\t\t\t\u003c/div\u003e\r\n\t\t\t\u003c/div\u003e\r\n\t\t\t\u003c!-- End: life time stats --\u003e\r\n\t\t\u003c/div\u003e\r\n\t\u003c/div\u003e\r\n\u003c/section\u003e\r\n\r\n\u003cstyle\u003e\r\n    table.additional {\r\n        padding-left: 5px;\r\n        padding-right: 5px;\r\n    }\r\n    \r\n\u003c/style\u003e\r\n\r\n\u003cscript type=\"text/javascript\"\u003e\r\n\r\n\r\nfunction basicsCtrl($scope, $rootScope, $http, $log, $stateParams, DMLService, RestApiService, UIService, PubSub, $location) {\r\n\r\n\r\n\r\n\r\n    $scope[\"main_contact_idEditCallBack\"] = function(data) {\r\n        $scope.detail[\"main_contact_id\"] = data.id;\r\n        $scope.edit();\r\n    }\r\n\r\n\r\n\r\n\r\n    $scope[\"img_file_idEditCallBack\"] = function(data) {\r\n        $scope.detail[\"img_file_id\"] = data.id;\r\n        $scope.edit();\r\n    }\r\n\r\n\r\n\r\n\r\n    $scope[\"company_work_idEditCallBack\"] = function(data) {\r\n        $scope.detail[\"company_work_id\"] = data.id;\r\n        $scope.edit();\r\n    }\r\n\r\n\r\n\r\n\r\n    $scope[\"manager_idEditCallBack\"] = function(data) {\r\n        $scope.detail[\"manager_id\"] = data.id;\r\n        $scope.edit();\r\n    }\r\n\r\n\r\n\r\n\r\n    $scope[\"gen